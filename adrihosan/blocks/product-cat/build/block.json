{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "doo-block/product-cat",
  "version": "0.1.0",
  "title": "Productos/subcategorías por Categoria",
  "category": "doo-blocks",
  "icon": "category",
  "description": "Bloque para mostrar categorías y productos",
  "supports": {
    "html": false,
    "align": true,
    "customClassName": true
  },
  "attributes": {
    "sel_categories": {
      "type": "array",
      "default": []
    },
    "variant": {
      "type": "string",
      "default": ""
    },
    "filters": {
      "type": "boolean",
      "default": false
    },
    "no_cached": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "doo",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php"
}